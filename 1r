#include <iostream>
using namespace std;
void rec(int* a, bool* used, int n, int k)
{
	if (k == n)
	{
		for (int i = 0; i < n; i++)
			cout << a[i] << ' ';
		cout << endl;
		return;
	}
	for (int i = n; i > 0; i--)
	{
		if (!used[i])
		{
			a[k] = i;
			used[i] = true;
			rec(a, used, n, k + 1);
			used[i] = false;
		}
	}
}
void main()
{
	int n;
	cin >> n;
		int* a = new int[n];
	bool* used = new bool[n + 1];
	for (int i = 1; i <= n; i++)
		used[i] = false;
	rec(a, used, n, 0);
}
